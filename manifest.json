{
  "manifest_version": 2,
  "name": "FormData Viewer",
  "description": "FormData Viewer",
  "version": "0.1",
  "browser_action": {
    "default_icon": "icon-switch.png",
    "default_title": "FormData Viewer",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [{
    "matches": ["http://*/*", "https://*/*"],
    "js": ["contentScript.js"]
  }]
}
